<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WeatherApp.Pages.Result"
             NavigationPage.HasNavigationBar="False">

    <StackLayout x:Name="stackLayout" VerticalOptions="Center" HorizontalOptions="Center" >
        <Label Text="Будильник сработает:" HorizontalOptions="Center"/>
        <Label x:Name="targetTime" HorizontalOptions="Center" />
        <Button x:Name="Back" Text="Назад" Clicked="back_Clicked" HorizontalOptions="Center"></Button>
    </StackLayout>

</ContentPage>